// Place your settings in this file to overwrite default and user settings.
{
  "prettier.bracketSpacing": true,
  "prettier.eslintIntegration": true,
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "eslint.enable": true,
  "editor.formatOnPaste": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "coverage": true,
    ".idea": true,
    "lib": true,
    "**/*.sublime-*": true,
    "node_modules": true,
    ".vscode": true
  }
}
